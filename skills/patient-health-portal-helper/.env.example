# Patient Health Portal Helper - Environment Variables
# Copy this file to .env and fill in your actual credentials

# Required - Your portal credentials
HEALTH_PORTAL_URL=https://patientconnect.shinepartners.ca/wctbhgzx00001tp1uz.mthd
HEALTH_PORTAL_TYPE=ShinePartners
HEALTH_PORTAL_USERNAME=your_username_here
HEALTH_PORTAL_PASSWORD=your_password_here

# Optional - Family attendees (comma-separated emails)
HEALTH_PORTAL_FAMILY_ATTENDEES=family1@example.com,family2@example.com

# Optional - Sync schedule (24-hour format, UTC)
HEALTH_PORTAL_SYNC_TIME=09:00

# Enable/disable the sync
HEALTH_PORTAL_ENABLED=true

# Optional - Log level (DEBUG, INFO, WARN, ERROR)
HEALTH_PORTAL_LOG_LEVEL=INFO
